{
  "name": "open-transit",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "server",
  "workspaceFolder": "/workspaces/open-transit",
  "runServices": [
    "db",
    "server"
  ],
  "shutdownAction": "stopCompose",
  "forwardPorts": [
    8000,
    5432
  ],
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    }
  },
  "postCreateCommand": "cd server && uv sync"
}
